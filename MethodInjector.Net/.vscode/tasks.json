{
    "version": "0.1.0",
    "command": "bash",
    "isShellCommand": true,
    "args": [ "-c" ],
    "tasks": [
        {
            "taskName": "build",
            "suppressTaskName": true,
            "args": [
                "dotnet build -c Release \"${workspaceFolder}\""
            ],
            "isBuildCommand": true,
            "problemMatcher": "$msCompile"
        },
        {
            "taskName": "run",
            "suppressTaskName": true,
            "args": [
                "dotnet run -c Release --project \"${workspaceFolder}\""
            ]
        }
    ]
}