{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/FrameworkPrototype.Net.sln",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "clean",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}/FrameworkPrototype.Net.sln",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "test DataAccess.Net",
            "command": "dotnet",
            "type": "process",
            "args": [
                "test",
                "${workspaceFolder}/DataAccess.Net.Tests/DataAccess.Net.Tests.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "group": "test",
            "problemMatcher": "$msCompile"
        },
        {
            "label": "run UnderstandingDependencyInjection.Net",
            "type": "shell",
            "command": "dotnet run --project \"${workspaceFolder}/UnderstandingDependencyInjection.Net/UnderstandingDependencyInjection.Net.csproj\"",
            "problemMatcher": []
        },
        {
            "label": "run WebAPIApplication.Net",
            "type": "shell",
            "command": "dotnet run --project \"${workspaceFolder}/WebAPIApplication.Net/WebAPIApplication.Net.csproj\"",
            "problemMatcher": []
        }
    ]
}